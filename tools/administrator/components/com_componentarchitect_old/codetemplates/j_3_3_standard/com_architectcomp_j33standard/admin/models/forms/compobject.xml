<form
    addfieldpath="/administrator/components/[%%com_architectcomp%%]/models/fields"
    addrulepath="/administrator/components/[%%com_architectcomp%%]/models/rules"

>
    <fieldset
        name="basic_details"
    >
        <field
		    name="id"
		    type="text"
            label="JGLOBAL_FIELD_ID_LABEL"
            description ="JGLOBAL_FIELD_ID_DESC"
			readonly="true"
			class="readonly"
            default="0"
		/>
        [%%IF INCLUDE_ASSETACL%%]
            [%%IF INCLUDE_ASSETACL_RECORD%%]
        <field
            name="asset_id"
            type="hidden"
            filter="unset"
        />
            [%%ENDIF INCLUDE_ASSETACL_RECORD%%]
        [%%ENDIF INCLUDE_ASSETACL%%]
        [%%IF INCLUDE_NAME%%]
        <field
		    name="name"
		    type="text"
			label="[%%COM_ARCHITECTCOMP%%]_FIELD_NAME_LABEL"
			description="[%%COM_ARCHITECTCOMP%%]_[%%COMPOBJECTPLURAL%%]_FIELD_NAME_DESC"
			size="50"
			required="true"
		/>
            [%%IF INCLUDE_ALIAS%%]
        <field
		    name="alias"
		    type="text"
			label="JFIELD_ALIAS_LABEL"
			description="[%%COM_ARCHITECTCOMP%%]_FIELD_ALIAS_DESC"
			size="50"
         />
            [%%ENDIF INCLUDE_ALIAS%%]
        [%%ENDIF INCLUDE_NAME%%]
        [%%IF INCLUDE_VERSIONS%%]
        <field
            name="version_note"
            type="text"
            label="JGLOBAL_FIELD_VERSION_NOTE_LABEL"
			description="JGLOBAL_FIELD_VERSION_NOTE_DESC"
			class="span12"
			size="45"
		/>
        [%%ENDIF INCLUDE_VERSIONS%%]
        [%%IF INCLUDE_STATUS%%]
        <field
		    name="state"
    		type="list"
			label="JSTATUS"
			description="[%%COM_ARCHITECTCOMP%%]_[%%COMPOBJECTPLURAL%%]_FIELD_STATUS_DESC"
			class="chzn-color-state"
			size="1"
			default="1"
		>
            <option value="1">JPUBLISHED</option>
            <option value="0">JUNPUBLISHED</option>
            <option value="2">JARCHIVED</option>
            <option value="-2">JTRASH</option>
        </field>
        [%%ENDIF INCLUDE_STATUS%%]
        [%%IF GENERATE_CATEGORIES%%]
        <field
            name="catid"
            type="categoryedit"
			label="JCATEGORY"
			description="JFIELD_CATEGORY_DESC"
            extension="[%%com_architectcomp%%]"
			required="true"
            addfieldpath="/administrator/components/com_categories/models/fields"
		>
        </field>
        [%%ENDIF GENERATE_CATEGORIES%%]
        [%%IF INCLUDE_TAGS%%]
        <field
            name="tags"
            type="tag"
            label="JTAG"
            description="JTAG_DESC"
            multiple="true"
		>
        </field>
        [%%ENDIF INCLUDE_TAGS%%]
        [%%IF INCLUDE_ACCESS%%]
        <field
            name="access"
			type="accesslevel"
			label="JFIELD_ACCESS_LABEL"
			description="JFIELD_ACCESS_DESC"
			size="1"
			default="1"
		/>
        [%%ENDIF INCLUDE_ACCESS%%]
        <field
            name="buttonspacer"
            type="spacer"
            label="JGLOBAL_ACTION_PERMISSIONS_LABEL"
            description="JGLOBAL_ACTION_PERMISSIONS_DESCRIPTION"
        />

        [%%IF INCLUDE_ORDERING%%]
        <field
            name="ordering"
			label="JFIELD_ORDERING_LABEL"
			description="JFIELD_ORDERING_DESC"
			type="[%%architectcomp%%]ordering"
			table="#__[%%architectcomp%%]_[%%compobjectplural%%]"
		/>
        [%%ENDIF INCLUDE_ORDERING%%]
        [%%IF INCLUDE_DESCRIPTION%%]
        <field
            [%%IF INCLUDE_INTRO%%]
		    name="introdescription"
            [%%ELSE INCLUDE_INTRO%%]
		    name="description"
            [%%ENDIF INCLUDE_INTRO%%]
            type="editor"
			label="JGLOBAL_DESCRIPTION"
            description="[%%COM_ARCHITECTCOMP%%]_[%%COMPOBJECTPLURAL%%]_FIELD_DESCRIPTION_DESC"
            filter="JComponentHelper::filterText"
            buttons="true"
            [%%IF GENERATE_PLUGINS%%]
                [%%IF GENERATE_PLUGINS_PAGEBREAK%%]
            hide="article"
                [%%ELSE GENERATE_PLUGINS_PAGEBREAK%%]
            hide="article"
                [%%ENDIF GENERATE_PLUGINS_PAGEBREAK%%]
            [%%ELSE GENERATE_PLUGINS%%]            
            hide="article,pagebreak"                
            [%%ENDIF GENERATE_PLUGINS%%] 
        />
        [%%ENDIF INCLUDE_DESCRIPTION%%]
        [%%IF INCLUDE_ASSETACL%%]
            [%%IF INCLUDE_ASSETACL_RECORD%%]
        <field
            name="rules"
            type="rules"
            label="JFIELD_RULES_LABEL"
     		translate_label="false"
			filter="rules"
			component="[%%com_architectcomp%%]"
			section="[%%compobject%%]"
			validate="rules"
		/>
            [%%ENDIF INCLUDE_ASSETACL_RECORD%%]
        [%%ENDIF INCLUDE_ASSETACL%%]
        [%%IF INCLUDE_FEATURED%%]
        <field
            name="featured"
			type="radio"
			label="JFEATURED"
			description="[%%COM_ARCHITECTCOMP%%]_[%%COMPOBJECTPLURAL%%]_FIELD_FEATURED_DESC"
		    class="btn-group btn-group-yesno"
			default="0"
		>
            <option	value="1">JYES</option>
            <option	value="0">JNO</option>
        </field>
        [%%ENDIF INCLUDE_FEATURED%%]
        [%%IF INCLUDE_LANGUAGE%%]
        <field
            name="language"
            type="contentlanguage"
            label="JFIELD_LANGUAGE_LABEL"
		    description="[%%COM_ARCHITECTCOMP%%]_[%%COMPOBJECTPLURAL%%]_FIELD_LANGUAGE_DESC"
			default="*"
		>
            <option value="*">JALL</option>
        </field>
        [%%ENDIF INCLUDE_LANGUAGE%%]
        [%%IF INCLUDE_HITS%%]
        <field
            name="hits"
            type="text"
            label="JGLOBAL_HITS"
			description="[%%COM_ARCHITECTCOMP%%]_[%%COMPOBJECTPLURAL%%]_FIELD_HITS_DESC"
			class="readonly"
			size="6"
			readonly="true"
			filter="unset"
		/>
        [%%ENDIF INCLUDE_HITS%%]
        [%%FOREACH OBJECT_FIELDSET%%]
        	[%%IF FIELDSET_BASIC_DETAILS%%]
        		[%%FOREACH OBJECT_FIELD%%]
        <field
            name="[%%FIELD_CODE_NAME%%]"
            type="[%%FIELD_TYPE%%]"
            label="[%%COM_ARCHITECTCOMP%%]_[%%COMPOBJECTPLURAL%%]_FIELD_[%%FIELD_CODE_NAME_UPPER%%]_LABEL"
            description="[%%COM_ARCHITECTCOMP%%]_[%%COMPOBJECTPLURAL%%]_FIELD_[%%FIELD_CODE_NAME_UPPER%%]_DESC"
            [%%FIELD_PARAMETERS%%]
        >
            [%%FIELD_OPTIONS%%]
        </field>  
                [%%ENDFOR OBJECT_FIELD%%]
            [%%ENDIF FIELDSET_BASIC_DETAILS%%]
        [%%ENDFOR OBJECT_FIELDSET%%]         
    </fieldset>
    [%%IF INCLUDE_IMAGE%%]
    <fields
        name="images"
        label="[%%COM_ARCHITECTCOMP%%]_[%%COMPOBJECTPLURAL%%]_IMAGE_OPTIONS"
    >
        <field
            name="image_url"
            type="media"
            label="[%%COM_ARCHITECTCOMP%%]_FIELD_IMAGE_LABEL"
            description="[%%COM_ARCHITECTCOMP%%]_[%%COMPOBJECTPLURAL%%]_FIELD_IMAGE_DESC"
        />
        <field
            name="image_alt_text"
            type="text"
            label="[%%COM_ARCHITECTCOMP%%]_FIELD_IMAGE_ALTTEXT_LABEL"
            description="[%%COM_ARCHITECTCOMP%%]_[%%COMPOBJECTPLURAL%%]_FIELD_IMAGE_ALTTEXT_DESC"
            size="20"
        />
        <field
            name="image_caption"
            type="text"
            label="[%%COM_ARCHITECTCOMP%%]_FIELD_IMAGE_CAPTION_LABEL"
            description="[%%COM_ARCHITECTCOMP%%]_[%%COMPOBJECTPLURAL%%]_FIELD_IMAGE_CAPTION_DESC"
            size="20"
            [%%IF OBJECT_READONLY%%]
            readonly="true"
            disabled="true"
            [%%ENDIF OBJECT_READONLY%%]
        />        
        [%%IF INCLUDE_INTRO%%]
        <field
            name="intro_image_url"
            type="media"
            label="[%%COM_ARCHITECTCOMP%%]_FIELD_INTRO_IMAGE_LABEL"
            description="[%%COM_ARCHITECTCOMP%%]_[%%COMPOBJECTPLURAL%%]_FIELD_INTRO_IMAGE_DESC"
        />
        <field name="intro_image_alt_text"
            type="text"
            label="[%%COM_ARCHITECTCOMP%%]_FIELD_INTRO_IMAGE_ALTTEXT_LABEL"
            description="[%%COM_ARCHITECTCOMP%%]_[%%COMPOBJECTPLURAL%%]_FIELD_IMAGE_ALTTEXT_DESC"
            size="20"
        />
        <field
            name="intro_image_caption"
            type="text"
            label="[%%COM_ARCHITECTCOMP%%]_FIELD_INTRO_IMAGE_CAPTION_LABEL"
            description="[%%COM_ARCHITECTCOMP%%]_[%%COMPOBJECTPLURAL%%]_FIELD_INTRO_IMAGE_CAPTION_DESC"
            size="20"
            [%%IF OBJECT_READONLY%%]
            readonly="true"
            disabled="true"
            [%%ENDIF OBJECT_READONLY%%]
        />
        [%%ENDIF INCLUDE_INTRO%%]
    </fields>
    [%%ENDIF INCLUDE_IMAGE%%]
    [%%IF INCLUDE_URLS%%]
    <fields
        name="urls"
        label="[%%COM_ARCHITECTCOMP%%]_[%%COMPOBJECTPLURAL%%]_URLS_OPTIONS"
     >
        <field
            name="urla"
            type="url"
            label="[%%COM_ARCHITECTCOMP%%]_FIELD_URLA_LABEL"
            description="[%%COM_ARCHITECTCOMP%%]_FIELD_URL_DESC"
            validate="url"
            filter="url"
       />
        <field
            name="urla_text"
            type="text"
            label="[%%COM_ARCHITECTCOMP%%]_FIELD_URLA_LINK_TEXT_LABEL"
            description="[%%COM_ARCHITECTCOMP%%]_FIELD_URL_LINK_TEXT_DESC"
            size="20"
        />
        <field
            name="urla_target"
            type="list"
            label="[%%COM_ARCHITECTCOMP%%]_FIELD_URLA_TARGET_LABEL"
            description="[%%COM_ARCHITECTCOMP%%]_FIELD_URL_TARGET_DESC"
			class="chzn-color"
            filter="options"
            default=""
        >
            <option value="">JGLOBAL_USE_GLOBAL</option>
            <option value="0">JBROWSERTARGET_PARENT</option>
            <option value="1">JBROWSERTARGET_NEW</option>
            <option value="2">JBROWSERTARGET_POPUP</option>
            <option value="3">JBROWSERTARGET_MODAL</option>
        </field>
        <field
            name="spacer3"
            type="spacer"
            hr="true"
				/>
        <field
            name="urlb"
            type="url"
            label="[%%COM_ARCHITECTCOMP%%]_FIELD_URLB_LABEL"
            description="[%%COM_ARCHITECTCOMP%%]_FIELD_URL_DESC"
            validate="url"
            filter="url"
        />
        <field
            name="urlb_text"
            type="text"
            label="[%%COM_ARCHITECTCOMP%%]_FIELD_URLB_LINK_TEXT_LABEL"
            description="[%%COM_ARCHITECTCOMP%%]_FIELD_URL_LINK_TEXT_DESC"
            size="20"
        />
        <field
            name="urlb_target"
            type="list"
            label="[%%COM_ARCHITECTCOMP%%]_FIELD_URLB_TARGET_LABEL"
            description="[%%COM_ARCHITECTCOMP%%]_FIELD_URL_TARGET_DESC"
			class="chzn-color"
            default=""
            filter="options"
        >
            <option value="">JGLOBAL_USE_GLOBAL</option>
            <option value="0">JBROWSERTARGET_PARENT</option>
            <option value="1">JBROWSERTARGET_NEW</option>
            <option value="2">JBROWSERTARGET_POPUP</option>
            <option value="3">JBROWSERTARGET_MODAL</option>
        </field>
        <field
            name="spacer4"
            type="spacer"
            hr="true"
				/>
        <field
            name="urlc"
            type="url"
            label="[%%COM_ARCHITECTCOMP%%]_FIELD_URLC_LABEL"
            description="[%%COM_ARCHITECTCOMP%%]_FIELD_URL_DESC"
            validate="url"
            filter="url"
        />
        <field
            name="urlc_text"
            type="text"
            label="[%%COM_ARCHITECTCOMP%%]_FIELD_URLC_LINK_TEXT_LABEL"
            description="[%%COM_ARCHITECTCOMP%%]_FIELD_URL_LINK_TEXT_DESC"
            size="20"
        />
        <field
            name="urlc_target"
            type="list"
            label="[%%COM_ARCHITECTCOMP%%]_FIELD_URLC_TARGET_LABEL"
            description="[%%COM_ARCHITECTCOMP%%]_FIELD_URL_TARGET_DESC"
			class="chzn-color"
            filter="options"
            default=""
        >
            <option value="">JGLOBAL_USE_GLOBAL</option>
            <option value="0">JBROWSERTARGET_PARENT</option>
            <option value="1">JBROWSERTARGET_NEW</option>
            <option value="2">JBROWSERTARGET_POPUP</option>
            <option value="3">JBROWSERTARGET_MODAL</option>
        </field>
    </fields>
    [%%ENDIF INCLUDE_URLS%%]
    [%%FOREACH OBJECT_FIELDSET%%]
        [%%IF FIELDSET_NOT_BASIC_DETAILS%%]
    <fieldset
        name="fieldset_[%%FIELDSET_CODE_NAME%%]"
        label="[%%COM_ARCHITECTCOMP%%]_[%%COMPOBJECTPLURAL%%]_FIELDSET_[%%FIELDSET_CODE_NAME_UPPER%%]_LABEL"
        description="[%%COM_ARCHITECTCOMP%%]_[%%COMPOBJECTPLURAL%%]_FIELDSET_[%%FIELDSET_CODE_NAME_UPPER%%]_DESC"
    >
            [%%FOREACH OBJECT_FIELD%%]
        <field
            name="[%%FIELD_CODE_NAME%%]"
            type="[%%FIELD_TYPE%%]"
            label="[%%COM_ARCHITECTCOMP%%]_[%%COMPOBJECTPLURAL%%]_FIELD_[%%FIELD_CODE_NAME_UPPER%%]_LABEL"
            description="[%%COM_ARCHITECTCOMP%%]_[%%COMPOBJECTPLURAL%%]_FIELD_[%%FIELD_CODE_NAME_UPPER%%]_DESC"
            [%%FIELD_PARAMETERS%%]
        >
            [%%FIELD_OPTIONS%%]
        </field>  
            [%%ENDFOR OBJECT_FIELD%%]
    </fieldset>
        [%%ENDIF FIELDSET_NOT_BASIC_DETAILS%%]
    [%%ENDFOR OBJECT_FIELDSET%%]
    [%%FOREACH REGISTRY_FIELD%%]
    <fields
        name="[%%FIELD_CODE_NAME%%]"
        label="[%%COM_ARCHITECTCOMP%%]_[%%COMPOBJECTPLURAL%%]_FIELD_[%%FIELD_CODE_NAME_UPPER%%]_LABEL"        
    >
    <fieldset
        name="[%%FIELD_CODE_NAME%%]"
        label="[%%COM_ARCHITECTCOMP%%]_[%%COMPOBJECTPLURAL%%]_FIELD_[%%FIELD_CODE_NAME_UPPER%%]_LABEL"
        description="[%%COM_ARCHITECTCOMP%%]_[%%COMPOBJECTPLURAL%%]_FIELD_[%%FIELD_CODE_NAME_UPPER%%]_DESC"
    >
        [%%FOREACH REGISTRY_ENTRY%%]
        <field
            name="[%%FIELD_CODE_NAME%%]"
            type="[%%FIELD_TYPE%%]"
            label="[%%COM_ARCHITECTCOMP%%]_[%%COMPOBJECTPLURAL%%]_FIELD_[%%FIELD_CODE_NAME_UPPER%%]_LABEL"
            description="[%%COM_ARCHITECTCOMP%%]_[%%COMPOBJECTPLURAL%%]_FIELD_[%%FIELD_CODE_NAME_UPPER%%]_DESC"
            [%%FIELD_PARAMETERS%%]
        >
            [%%FIELD_OPTIONS%%]
        </field>  
        [%%ENDFOR REGISTRY_ENTRY%%]
    </fieldset>
    </fields>
    [%%ENDFOR REGISTRY_FIELD%%]
    <fieldset
        name="publishing"
		label="[%%COM_ARCHITECTCOMP%%]_FIELDSET_PUBLISHING_LABEL"
    >
        [%%IF INCLUDE_PUBLISHED_DATES%%]
        <field
            name="publish_up"
            type="calendar"
			label="[%%COM_ARCHITECTCOMP%%]_FIELD_PUBLISH_UP_LABEL"
			description="[%%COM_ARCHITECTCOMP%%]_[%%COMPOBJECTPLURAL%%]_FIELD_PUBLISH_UP_DESC"
			size="22"
			format="%Y-%m-%d %H:%M:%S"
			filter="user_utc"
		/>
        <field
            name="publish_down"
            type="calendar"
			label="[%%COM_ARCHITECTCOMP%%]_FIELD_PUBLISH_DOWN_LABEL"
			description="[%%COM_ARCHITECTCOMP%%]_[%%COMPOBJECTPLURAL%%]_FIELD_PUBLISH_DOWN_DESC"
			size="22"
			format="%Y-%m-%d %H:%M:%S"
			filter="user_utc"
		/>
        [%%ENDIF INCLUDE_PUBLISHED_DATES%%]
        [%%IF INCLUDE_CREATED%%]
        <field
            name="created_by"
            type="user"
			label="[%%COM_ARCHITECTCOMP%%]_FIELD_CREATED_BY_LABEL"
			description="[%%COM_ARCHITECTCOMP%%]_[%%COMPOBJECTPLURAL%%]_FIELD_CREATED_BY_DESC"
		/>
        <field
            name="created"
            type="calendar"
            label="[%%COM_ARCHITECTCOMP%%]_FIELD_CREATED_LABEL"
			description="[%%COM_ARCHITECTCOMP%%]_[%%COMPOBJECTPLURAL%%]_FIELD_CREATED_DESC"
			size="22"
			format="%Y-%m-%d %H:%M:%S"
			filter="user_utc"
            [%%IF OBJECT_READONLY%%]
			readonly="true"
			disabled="true"
            [%%ENDIF OBJECT_READONLY%%]
		/>
        <field
			id="created_by_alias"
			name="created_by_alias"
			type="text"
			label="[%%COM_ARCHITECTCOMP%%]_FIELD_CREATED_BY_ALIAS_LABEL"
			description="[%%COM_ARCHITECTCOMP%%]_[%%COMPOBJECTPLURAL%%]_FIELD_CREATED_BY_ALIAS_DESC"
            size="20"
		/>
        [%%ENDIF INCLUDE_CREATED%%]
        [%%IF INCLUDE_MODIFIED%%]
        <field
            name="modified"
            type="calendar"
			label="[%%COM_ARCHITECTCOMP%%]_FIELD_MODIFIED_LABEL"
			description="[%%COM_ARCHITECTCOMP%%]_[%%COMPOBJECTPLURAL%%]_FIELD_MODIFIED_DESC"
            class="readonly"
			size="22"
			readonly="true"
			format="%Y-%m-%d %H:%M:%S"
			filter="user_utc"
		/>
        <field
            name="modified_by"
            type="user"
		    label="[%%COM_ARCHITECTCOMP%%]_FIELD_MODIFIED_BY_LABEL"
		    class="readonly"
		    readonly="true"
		    filter="unset"
		/>
        [%%ENDIF INCLUDE_MODIFIED%%]
        [%%IF INCLUDE_VERSIONS%%]
        <field
            name="version"
            type="text"
			label="[%%COM_ARCHITECTCOMP%%]_FIELD_VERSION_LABEL"
            description="[%%COM_ARCHITECTCOMP%%]_[%%COMPOBJECTPLURAL%%]_FIELD_VERSION_DESC"
			class="readonly"
			size="6"
			readonly="true"
			filter="unset"
		/>
        [%%ENDIF INCLUDE_VERSIONS%%]
        [%%IF INCLUDE_CHECKOUT%%]
        <field
            name="checked_out"
			type="hidden"
			filter="unset"
		/>
        <field
            name="checked_out_time"
			type="hidden"
			filter="unset"
		 />
        [%%ENDIF INCLUDE_CHECKOUT%%]
    </fieldset>
    [%%IF INCLUDE_METADATA%%]
    <fieldset
        name="metadata"
        label="[%%COM_ARCHITECTCOMP%%]_FIELDSET_METADATA_LABEL"
    >
        <field
            name="metakey"
            type="textarea"
            label="JFIELD_META_KEYWORDS_LABEL"
            description="JFIELD_META_KEYWORDS_DESC"
            rows="3"
            cols="35"
		/>
        <field
            name="metadesc"
            type="textarea"
            label="JFIELD_META_DESCRIPTION_LABEL"
            description="JFIELD_META_DESCRIPTION_DESC"
            rows="3"
            cols="35"
		 />
        <field
            name="robots"
            type="list"
            label="JFIELD_METADATA_ROBOTS_LABEL"
            description="JFIELD_METADATA_ROBOTS_DESC"
			class="chzn-color"
		>
            <option value="">JGLOBAL_USE_GLOBAL</option>
            <option value="index, follow">JGLOBAL_INDEX_FOLLOW</option>
            <option value="noindex, follow">JGLOBAL_NOINDEX_FOLLOW</option>
            <option value="index, nofollow">JGLOBAL_INDEX_NOFOLLOW</option>
            <option value="noindex, nofollow">JGLOBAL_NOINDEX_NOFOLLOW</option>
        </field>
        <field
            name="author"
            type="text"
			label="JAUTHOR"
			description="JFIELD_METADATA_AUTHOR_DESC"
			size="20"
		/>
        <field
            name="xreference"
            type="text"
            label="JFIELD_KEY_REFERENCE_LABEL"
            description="JFIELD_KEY_REFERENCE_DESC"
            size="50"
        />
    </fieldset>
    [%%ENDIF INCLUDE_METADATA%%]
    [%%IF GENERATE_SITE%%]
        [%%IF INCLUDE_PARAMS_RECORD%%]
    <fields
        name="params"
        label="[%%COM_ARCHITECTCOMP%%]_[%%COMPOBJECTPLURAL%%]_PARAMS_OPTIONS"        
    >
        <fieldset
            name="basic"
            label="[%%COM_ARCHITECTCOMP%%]_[%%COMPOBJECTPLURAL%%]_FIELDSET_LAYOUT"
        >
            <field
				name="show_[%%compobject%%]_icons"
				type="radio"
				label="[%%COM_ARCHITECTCOMP%%]_SHOW_ICONS_LABEL"
				description="[%%COM_ARCHITECTCOMP%%]_SHOW_ICONS_DESC"
                class="btn-group"
			>
                [%%IF INCLUDE_PARAMS_GLOBAL%%]
                <option value="">JGLOBAL_USE_GLOBAL</option>
                [%%ENDIF INCLUDE_PARAMS_GLOBAL%%]
                <option	value="0">[%%COM_ARCHITECTCOMP%%]_SHOW_ICONS_VALUE_TEXT</option>
                <option	value="1">[%%COM_ARCHITECTCOMP%%]_SHOW_ICONS_VALUE_ICONS</option>
                <option	value="-1">JHIDE</option>
            </field>
            <field
				name="show_[%%compobject%%]_print_icon"
				type="radio"
				label="[%%COM_ARCHITECTCOMP%%]_SHOW_PRINT_ICON_LABEL"
				description="[%%COM_ARCHITECTCOMP%%]_SHOW_PRINT_ICON_DESC"
                class="btn-group"
			>
                [%%IF INCLUDE_PARAMS_GLOBAL%%]
                <option value="">JGLOBAL_USE_GLOBAL</option>
                [%%ENDIF INCLUDE_PARAMS_GLOBAL%%]
                <option	value="0">JHIDE</option>
                <option	value="1">JSHOW</option>
            </field>
            <field
				name="show_[%%compobject%%]_email_icon"
				type="radio"
				label="[%%COM_ARCHITECTCOMP%%]_SHOW_EMAIL_ICON_LABEL"
				description="[%%COM_ARCHITECTCOMP%%]_[%%COMPOBJECTPLURAL%%]_SHOW_EMAIL_ICON_DESC"
                class="btn-group"
			>
                [%%IF INCLUDE_PARAMS_GLOBAL%%]
                <option value="">JGLOBAL_USE_GLOBAL</option>
                [%%ENDIF INCLUDE_PARAMS_GLOBAL%%]
                <option	value="0">JHIDE</option>
                <option	value="1">JSHOW</option>
            </field>
            <field
