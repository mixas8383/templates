<form
    addrulepath="/administrator/components/com_componentarchitect/models/rules"
>
    <field
        name="component_name"
        type="text"
        label="COM_COMPONENTARCHITECT_COMPONENT_WIZARD_COMPONENT_NAME_LABEL"
        description="COM_COMPONENTARCHITECT_GENERATE_COMPONENT_DESC"
        class="inputbox validate-componentname"
        size="50"
        validate="componentname"
        required="true"
	/>
    <field
        name="component_objects"
        type="text"
        label="COM_COMPONENTARCHITECT_COMPONENT_WIZARD_COMPONENT_OBJECTS_LABEL"
        description="COM_COMPONENTARCHITECT_COMPONENT_WIZARD_COMPONENT_OBJECTS_DESC"
        class="inputbox validate-componentobjectname"
        size="50"
        validate="componentobjectname"
	/>
</form>
